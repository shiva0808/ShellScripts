cut -c 1 <file>       Print first character in each line
cut -c 5-10     Print from Character 5 to 10
cut -c 1-5      Print from 1 to 5
cut -c 5-       print from 5 onwards
cut 1,3,5       Print 1character 3character 5character


echo "one\ttwo\tthree" | cut -f 1
echo "one,two,three" | cut -d "," -f 1
cut -d ":" -f 1,3 --output-delimiter=',' /etc/password
cut -d "," -f 1,3 /etc/password | grep -v "user,password"
  
